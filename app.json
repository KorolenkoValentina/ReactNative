{
  "expo": {
    "name": "AwesomeProject",
    "slug": "AwesomeProject",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "description": "App Pizza",
      "image": "./assets/splash-image.jpg",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      }
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      [
        "expo-font",
        {
          "fonts": [
            {
              "fonts": "assets/fonts/Poppins-Regular.ttf"
              
            },
            {
              "fonts": "assets/fonts/Poppins-SemiBold.ttf"
              
            },
            {
              "fonts": "assets/fonts/Poppins-Bold.ttf"
             
            },
            {
              "fonts": "assets/fonts/Lemon-Regular.ttf"
              
            }
            
          ]
        }
      ]
    ]
  
  }
}
